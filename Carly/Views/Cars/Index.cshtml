@model IEnumerable<Carly.Models.Car>
@{
    ViewBag.Title = "CarsList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>CarsList</h2>

@if (Model.Count() == 0)
{
    <p>No car available for now</p>
}
else
{
    <table class="table table-bordered table-hover">
        <thead>
            <tr>
                <th>Car</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var car in Model)
            {
                <tr>
                    <td>@car.Name</td>
                </tr>
            }
        </tbody>
    </table>

}

